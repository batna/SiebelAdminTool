bdd:
    pattern:  /bdd
    defaults: { _controller: "BatnaArchiSiebelBundle:Bdd:index" }

bdd_show:
    pattern:  /bdd/{id}/show
    defaults: { _controller: "BatnaArchiSiebelBundle:Bdd:show" }

bdd_new:
    pattern:  /bdd/new
    defaults: { _controller: "BatnaArchiSiebelBundle:Bdd:new" }

bdd_create:
    pattern:  /bdd/create
    defaults: { _controller: "BatnaArchiSiebelBundle:Bdd:create" }
    requirements: { _method: post }

bdd_edit:
    pattern:  /bdd/{id}/edit
    defaults: { _controller: "BatnaArchiSiebelBundle:Bdd:edit" }

bdd_update:
    pattern:  /bdd/{id}/update
    defaults: { _controller: "BatnaArchiSiebelBundle:Bdd:update" }
    requirements: { _method: post }

bdd_delete:
    pattern:  /bdd/{id}/delete
    defaults: { _controller: "BatnaArchiSiebelBundle:Bdd:delete" }
    requirements: { _method: post }

